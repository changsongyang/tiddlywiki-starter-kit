{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "curl -fsSL https://bun.sh/install | bash && export BUN_INSTALL='$HOME/.bun' && export PATH=$BUN_INSTALL/bin:$PATH",
  "buildCommand": "~/.bun/bin/bun run build --production",
  "outputDirectory": ".tiddlywiki",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  }
}
