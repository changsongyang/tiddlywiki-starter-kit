{
  "alias": "neotw",
  "outputDirectory": "dist",
  "build": {
    "command": "yarn run build:oeyoews",
    "output": "dist"
  },
  "installCommand": "yarn workspaces focus --all --production",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "production": {
    "branch": "main"
  },
  "preview": {
    "branch": "dev"
  },
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  }
}
