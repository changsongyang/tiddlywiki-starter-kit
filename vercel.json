{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": ".tiddlywiki",
  "installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install --production",
  "buildCommand": "~/.bun/bin/bun run build --production",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  }
}
