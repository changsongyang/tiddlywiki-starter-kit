{
  "outputDirectory": ".tiddlywiki",
  "installCommand": "pnpm i --frozen-lockfile --prod",
  "buildCommand": "curl -fsSL https://bun.sh/install | bash && bun run build",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  }
}
