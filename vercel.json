{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "curl -fsSL https://bun.sh/install | bash && /vercel/.bun/bin/bun install",
  "buildCommand": "/vercel/.bun/bin/bun run build",
  "outputDirectory": ".tiddlywiki",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  }
}
